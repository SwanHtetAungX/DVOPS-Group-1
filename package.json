{
  "name": "dvops-final",
  "version": "1.0.0",
  "description": "Repository For final project",
  "main": "index.js",
  "nyc": {
    "include": [
      "utils/ExpenseUtil.js",
      "utils/TransactionUtil.js",
      "utils/UserUtil.js"
    ],
    "exclude": [],
    "reporter": [
      "html",
      "text-summary"
    ],
    "all": true,
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  },
  "mocha": {
    "reporter": "mochawesome"
  },
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "nyc mocha --watch tests/*.test.js --reporter mochawesome",
    "test-report": "nyc cypress run --spec 'public/js/*.cy.js' --reporter mochawesome",
    "test-reports": "nyc mocha tests/*.test.js --reporter mochawesome",
    "test:open": "nyc cypress open --reporter mochawesome",
    "commit": "cz",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "chai": "^4.3.10",
    "cypress": "^13.6.3",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "mocha": "^10.2.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.19",
    "babel-plugin-istanbul": "^6.1.1",
    "commitizen": "^4.3.0",
    "cypress-mochawesome-reporter": "^3.8.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "mochawesome": "^7.1.3",
    "nyc": "^15.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "./.husky/prepare-commit-msg.ps1"
    }
  }
}
